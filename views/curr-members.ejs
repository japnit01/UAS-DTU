<style>
    * {
        box-sizing: border-box;
    }
    nav.transparent {
        position: fixed !important;
        top: 0;
        background-color: #000000 !important;
    }
    .outer {
        display: flex;
        display: -moz-flex;
        display: -webkit-flex;
        flex-wrap: wrap;
        justify-content: space-around;
    }
    .box2 {
        display: flex;
        display: -moz-flex;
        display: -webkit-flex;
        margin: 50px 0;
        border-radius: 60px;
        background-color: #ffffff;
    }
    .box2 img {
        transform: scale(1.33);
    }
    .box2 div {
        font-size: 24px;
        padding: 10px 40px;
    }
    .box1, .box3, .box4 {
        text-align: center;
        box-sizing: border-box;
    }
    h3.center {
        text-align: center;
    }
    .box1, .box4 {
        background-color: #ffffff;
        margin: 80px 20px 30px 20px;
    }
    .box3 {
        margin: 50px 30px 0 30px;
    }
</style>

